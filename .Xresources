!
! .Xresources
!

!
! xterm
!

XTerm.termName:                 xterm-256color
XTerm.vt100.locale:             false
XTerm.vt100.utf8:               true
! Make 'Alt' key behave as on other terminal emulators
XTerm.vt100.metaSendsEscape:    true
XTerm.vt100.saveLines:          4096
XTerm.vt100.scrollBar:          false
XTerm.vt100.selectToClipboard:  true
XTerm.vt100.faceName:           Iosevka
XTerm.vt100.faceSize:           14
XTerm.vt100.cursorBlink:        false
XTerm.vt100.cursorUnderLine:    false
XTerm.vt100.OmitTranslation:    fullscreen
XTerm.vt100.translations:       #override \n \
    Alt <Key>C:                     copy-selection(CLIPBOARD) \n \
    Alt <Key>V:                     insert-selection(CLIPBOARD) \n \
    Alt <KeyPress>J:                scroll-forw(1, line) \n \
    Alt <KeyPress>D:                scroll-forw(1, halfpage) \n \
    Alt <KeyPress>K:                scroll-back(1, line) \n \
    Alt <KeyPress>U:                scroll-back(1, halfpage) \n \
    Alt <Key>minus:                 smaller-vt-font() \n \
    Alt <Key>plus:                  larger-vt-font() \n \
    Alt <Key>F:                     set-vt-font(d) \n \

!
! xidle
!

XIdle*position: no
XIdle*delay:    2

!
! xlock
!

XLock.dpmsoff:     1
XLock.description: off
XLock.sound:       off
XLock.echokeys:    on
XLock.echokey:     *
XLock.info:
XLock.mode:        blank
XLock.allowroot:   off
XLock.username:    Login:\040\040\040\040
XLock.password:    Password:
XLock.invalid:     Authorization Failed
XLock.timeout:     5
XLock.font:        -misc-fixed-medium-r-normal-*-20-*-*-*-*-*-iso10646-1
XLock.planfont:    -misc-fixed-medium-r-normal-*-20-*-*-*-*-*-iso10646-1
XLock.background:  #282828
XLock.foreground:  #ebdbb2

!
! Gruvbox dark theme
!

*.background: #282828
*.foreground: #ebdbb2
! Black + DarkGrey
*color0:      #282828
*color8:      #928374
! DarkRed + Red
*color1:      #cc241d
*color9:      #fb4934
! DarkGreen + Green
*color2:      #98971a
*color10:     #b8bb26
! DarkYellow + Yellow
*color3:      #d79921
*color11:     #fabd2f
! DarkBlue + Blue
*color4:      #458588
*color12:     #83a598
! DarkMagenta + Magenta
*color5:      #b16286
*color13:     #d3869b
! DarkCyan + Cyan
*color6:      #689d6a
*color14:     #8ec07c
! LightGrey + White
*color7:      #a89984
*color15:     #ebdbb2

!
! Miscellaneous
!

Sxiv.font: xft:Iosevka:size=12:antialias=true:hinting=true

Xcursor.theme: Paper
Xcursor.size: 25

